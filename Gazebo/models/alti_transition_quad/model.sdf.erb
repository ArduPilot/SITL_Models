  <plugin name="ArduPilotPlugin" filename="ArduPilotPlugin">
    <!-- Port settings -->
    <fdm_addr><%= fdm_addr %></fdm_addr>
    <fdm_port_in><%= fdm_port_in %></fdm_port_in>
    <connectionTimeoutMaxCount>5</connectionTimeoutMaxCount>
    <lock_step>1</lock_step>

    <modelXYZToAirplaneXForwardZDown degrees="true">0 0 0 180 0 0</modelXYZToAirplaneXForwardZDown>
    <gazeboXYZToNED degrees="true">0 0 0 180 0 90</gazeboXYZToNED>

    <!-- Sensors -->
    <imuName>imu_sensor</imuName>

    <!-- 
        SERVO1_FUNCTION   4 (Aileron)
        SERVO1_MAX        2000
        SERVO1_MIN        1000
        SERVO1_REVERSED   0
        SERVO1_TRIM       1500

        pwm:          =>  [1100, 1900] 
        input:        =>  [0, 1]
        offset: -0.5  =>  [-0.5, 0.5]
        scale:   2.0  =>  [-1.0, 1.0]
        scale: 0.524  =>  [-0.524, 0.524]
      -->
    <control channel="0">
      <jointName>left_aileron_joint</jointName>
      <useForce>1</useForce>
      <multiplier>-0.872664626</multiplier>
      <offset>-0.5</offset>
      <servo_min>1000</servo_min>
      <servo_max>2000</servo_max>
      <type>POSITION</type>
      <p_gain>10.0</p_gain>
      <i_gain>0</i_gain>
      <d_gain>0</d_gain>
      <i_max>0</i_max>
      <i_min>0</i_min>
      <cmd_max>2.5</cmd_max>
      <cmd_min>-2.5</cmd_min>
    </control>

    <control channel="0">
      <jointName>right_aileron_joint</jointName>
      <useForce>1</useForce>
      <multiplier>0.872664626</multiplier>
      <offset>-0.5</offset>
      <servo_min>1000</servo_min>
      <servo_max>2000</servo_max>
      <type>POSITION</type>
      <p_gain>10.0</p_gain>
      <i_gain>0</i_gain>
      <d_gain>0</d_gain>
      <i_max>0</i_max>
      <i_min>0</i_min>
      <cmd_max>2.5</cmd_max>
      <cmd_min>-2.5</cmd_min>
    </control>

    <!-- 
        SERVO2_FUNCTION   19 (Elevator)
        SERVO2_MAX        2000
        SERVO2_MIN        1000
        SERVO2_REVERSED   0
        SERVO2_TRIM       1500
      -->
    <control channel="1">
      <jointName>elevator_joint</jointName>
      <useForce>1</useForce>
      <multiplier>0.872664626</multiplier>
      <offset>-0.5</offset>
      <servo_min>1000</servo_min>
      <servo_max>2000</servo_max>
      <type>POSITION</type>
      <p_gain>10.0</p_gain>
      <i_gain>0</i_gain>
      <d_gain>0</d_gain>
      <i_max>0</i_max>
      <i_min>0</i_min>
      <cmd_max>2.5</cmd_max>
      <cmd_min>-2.5</cmd_min>
    </control>

    <!-- 
        SERVO3_FUNCTION   70 (Throttle)
        SERVO3_MAX        2000
        SERVO3_MIN        1000
        SERVO3_REVERSED   0
        SERVO3_TRIM       1000

        incoming control command [0, 1]
        so offset it by 0 to get [0, 1]
        and divide max target by 1.
        offset = 0
        multiplier = 838 max rpm / 1 = 838
      -->
    <control channel="2">
      <jointName>motor_f_joint</jointName>
      <useForce>1</useForce>
      <multiplier>-1500</multiplier>
