      <!-- 
          SERVO1_FUNCTION   4 (Aileron)
          SERVO1_MAX        2000
          SERVO1_MIN        1000
          SERVO1_REVERSED   0
          SERVO1_TRIM       1500

          pwm:          =>  [1000, 2000]
          input:        =>  [0, 1]
          offset: -0.5  =>  [-0.5, 0.5]
          scale:   2.0  =>  [-1.0, 1.0]
          scale: 0.524  =>  [-0.524, 0.524]
       -->
      <control channel="0">
        <jointName>left_aileron_joint</jointName>
        <multiplier>-0.8377580409572782</multiplier>
        <offset>-0.5</offset>
        <servo_min>1000</servo_min>
        <servo_max>2000</servo_max>
        <type>COMMAND</type>
        <cmd_topic>/left_aileron_joint/cmd_pos</cmd_topic>
