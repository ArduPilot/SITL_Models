    <plugin name="ArduPilotPlugin" filename="libArduPilotPlugin.so">
      <!-- Port settings -->
      <fdm_addr>127.0.0.1</fdm_addr>
      <fdm_port_in>9002</fdm_port_in>
      <connectionTimeoutMaxCount>5</connectionTimeoutMaxCount>
      <lock_step>1</lock_step>

      <!-- Frame conventions
        modelXYZToAirplaneXForwardZDown:
          - transforms body frame from orientation in Gazebo to NED

        gazeboXYZToNED
          - transforms world from Gazebo convention xyz = N -E -D
            to ArduPilot convention xyz = NED

        Vehicle is oriented x-forward, y-left, z-up
      -->
      <modelXYZToAirplaneXForwardZDown>0 0 0 3.141593 0 0</modelXYZToAirplaneXForwardZDown>
      <gazeboXYZToNED>0 0 0 3.141593 0 0</gazeboXYZToNED>

      <!-- Sensors -->
      <imuName>imu_sensor</imuName>

      <!-- 
          SERVO1_FUNCTION   77 (Elevon Left)
          SERVO1_MAX        2000
          SERVO1_MIN        1000
          SERVO1_REVERSED   0
          SERVO1_TRIM       1500

          pwm:          =>  [1000, 2000] 
          input:        =>  [0, 1]
          offset: -0.5  =>  [-0.5, 0.5]
          scale:   2.0  =>  [-1.0, 1.0]
          scale: 0.524  =>  [-0.524, 0.524]
       -->
      <control channel="0">
        <jointName>elevon_left_joint</jointName>
        <useForce>1</useForce>
        <multiplier>1.047</multiplier>
        <offset>-0.5</offset>
        <servo_min>1000</servo_min>
        <servo_max>2000</servo_max>
        <type>POSITION</type>
        <p_gain>5.0</p_gain>
        <i_gain>0</i_gain>
        <d_gain>0</d_gain>
        <i_max>-1</i_max>
        <i_min>0</i_min>
        <cmd_max>2.5</cmd_max>
        <cmd_min>-2.5</cmd_min>
      </control>

      <!-- 
          SERVO2_FUNCTION   78 (Elevon Right)
          SERVO2_MAX        2000
          SERVO2_MIN        1000
          SERVO2_REVERSED   0
          SERVO2_TRIM       1500
       -->
